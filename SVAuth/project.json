{
  "dependencies": {
    "Microsoft.NETCore.App": {
      "version": "1.0.0",
      "type": "platform"
    },
    "SVX": "1.0.0-*",
    "Microsoft.AspNetCore.Diagnostics": "1.0.0",
    "Microsoft.AspNetCore.Routing": "1.0.0",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0",
    "Microsoft.AspNetCore.Server.Kestrel.Https": "1.0.0",
    "Newtonsoft.Json": "9.0.1",
    "System.Diagnostics.TraceSource": "4.0.0",
    "System.Runtime.Serialization.Primitives": "4.1.1",
    "JwtCore": "1.1.0"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dotnet5.6",
        "dnxcore50",
        "portable-net45+win8"
      ]
    }
  },

  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true,
    "compile": {
      "exclude": [
        "ServiceProviders\\anyServiceProviderYouWantToExclude\\anyServiceProviderYouWantToExclude.cs"
      ]
    }
  },

  "runtimeOptions": {
    "gcServer": true
  },

  "tooling": {
    "defaultNamespace": "SVAuth"
  }
}
